{
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Content-Security-Policy", "value": "frame-ancestors 'self' https://*.kinobox.cz https://kinobox.cz *" },
        { "key": "X-Frame-Options", "value": "ALLOWALL" }
      ]
    }
  ]
}